    "/v1/syn/delivery/deliveryOrder": {
      "post": {
        "tags": ["生成寄送单通用接口"],
        "summary": "生成待寄送接口",
        "description": "",
        "consumes": ["application/json"],
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "description": "Authorization (Only:undefined)",
            "required": true,
            "type": "string"
          },
          {
            "name": "appid",
            "in": "header",
            "description": "appid (Only:undefined)",
            "required": true,
            "type": "string"
          },
          {
            "name": "bgrul",
            "in": "header",
            "description": "bgrul (Only:undefined)",
            "required": true,
            "type": "string"
          },
