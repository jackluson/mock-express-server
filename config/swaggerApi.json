            "name": "seq",
            "in": "header",
            "description": "seq (Only:undefined)",
            "required": true,
            "type": "string"
          },
          {
            "name": "sign",
            "in": "header",
            "description": "sign (Only:undefined)",
            "required": true,
            "type": "string"
          },
          {
            "name": "root",
            "in": "body",
            "schema": {
              "type": "object",
              "required": [
                "addressDetail",
                "appId",
                "city",
                "classType",
                "consignee",
                "countryCode",
                "countryName",
                "goods",
                "mobile",
                "province",
                "userId"
              ],
              "properties": {
                "addressDetail": {
                  "type": "string",
                  "description": "详细地址"
                },
                "appId": {
                  "type": "string",
                  "description": "所属appId，长度限制100字母，调用方如怕重复创建，appId后可附加业务唯一标识：格式4455254785__128，双下划线前是appid， 双下滑线后为业务唯一标识，如有重复，系统会提示重复 "
                },
                "area": {
                  "type": "string",
                  "description": "区"
                },
                "businessKey": {
                  "type": "string",
                  "description": "合同订单号"
                },
                "city": {
                  "type": "string",
                  "description": "市"
                },
                "classType": {
                  "type": "integer",
                  "format": "int32",
                  "description": "classType 258：数学思维2019，748：数学思维2020 2：轻课 3：礼品"
                },
                "consignee": {
                  "type": "string",
                  "description": "收件人名称"
                },
                "countryCode": {
                  "type": "string",
                  "description": "国家区号如86是中国"
                },
                "countryName": {
                  "type": "string",
                  "description": "国家名称"
                },
                "goods": {
                  "type": "array",
                  "description": "商品信息",
                  "items": {
                    "type": "object",
                    "required": ["code", "name"],
                    "properties": {
                      "code": {
                        "type": "string",
                        "description": "寄送物品编码"
                      },
                      "name": {
                        "type": "string",
                        "description": "寄送物品名称"
                      }
                    },
                    "title": "商品信息",
                    "$$ref": "#/definitions/商品信息"
                  }
                },
                "mobile": {
                  "type": "string",
                  "description": "收件人手机号"
                },
                "province": {
                  "type": "string",
                  "description": "省份"
                },
                "remark": {
                  "type": "string",
                  "description": "备注，长度限制：纯汉字100个，纯字母300个"
                },
                "userId": {
                  "type": "integer",
                  "format": "int64",
                  "description": "用户ID"
                }
              },
              "title": "入参",
              "$$ref": "#/definitions/入参"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "code": {
                  "type": "integer",
                  "format": "int32",
                  "description": "业务响应的数字状态码"
                },
                "count": {
                  "type": "integer",
                  "format": "int64",
                  "description": "总记录数"
                },
                "data": {
                  "type": "object",
                  "description": "请求所需的业务数据",
                  "properties": {}
                },
                "message": {
                  "type": "string",
                  "description": "业务响应的消息文本字符串"
                }
              },
